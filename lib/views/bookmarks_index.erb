<ul>
  <% @book.each do |k| %>
    <li>
      <a href = "<%= k.url %>">
        <%= k.title %>
      </a>
      <form action="/bookmarks/<%= k.id %>" method="post">
        <input type='hidden' name='_method' value='DELETE'/>
        <input type="submit" value="Delete" />
      </form>
      <form action="/bookmarks/<%= k.id %>/edit" method="get">
        <input type="submit" value="Edit" />
      </form>
    </li>
  <% end %>
</ul>
